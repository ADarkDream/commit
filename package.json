{
  "name": "commit",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "check": "eslint .",
    "check:prettier": "prettier --check .",
    "check:type": "vue-tsc --build --noEmit --force",
    "commit": "pnpm run lint:diff && cz",
    "lint": "eslint . --fix",
    "lint-staged": "lint-staged",
    "lint:commitlint": "commitlint --edit $1",
    "lint:diff": "git add . && pnpm exec lint-staged",
    "lint:prettier": "prettier . --write",
    "lint:settings": "npx @eslint/config-inspector@latest"
  },
  "lint-staged": {
    "**/*.{cjs,mjs,js,jsx,tsx,ts,less,md,json}": [
      "pnpm exec eslint --fix",
      "prettier . --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@eslint/js": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^60.8.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "typescript-eslint": "^8.46.3",
    "vue-eslint-parser": "^9.4.3"
  }
}
